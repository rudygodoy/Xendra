from (

select
il.m_product_id as product_id,
movementqty as qtyentered,
(
	case when w.m_warehouse_id = 1004415
		then movementqty
	else
		0
	end
) as LEDUX,

*
from m_productionline  il
join m_productionplan i on il.m_productionplan_id = i.m_productionplan_id and i.isactive  = 'Y'
join m_production pp on pp.m_production_id = i.m_production_id
JOIN m_warehouse w ON w.m_warehouse_id = (( SELECT loc.m_warehouse_id FROM m_locator loc WHERE loc.m_locator_id = il.m_locator_id))
where il.isactive='Y' and (pp.docstatus = ANY (ARRAY['CO'::bpchar, 'CL'::bpchar])) and pp.isactive='Y'

) xx
group by product_id
) total_agrupador
group by product_id

) datos_producto
     left join m_product prod on product_id = prod.m_product_id
     left join "m_product_group" m_product_group ON prod."m_product_group_id" = m_product_group."m_product_group_id"
     left join "m_product_brand" m_product_brand ON prod."m_product_brand_id" = m_product_brand."m_product_brand_id"
     left join "m_product_type" m_product_type ON prod."m_product_type_id" = m_product_type."m_product_type_id"
     left join "m_product_generic" m_product_generic ON prod."m_product_generic_id" = m_product_generic."m_product_generic_id"
	where prod.ad_client_id = $P{AD_Client_ID} AND PROD.AD_ORG_ID = $P{AD_Org_ID}
