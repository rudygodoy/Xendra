	then qtyordered
	else 0
end
) as porentregar_0015,
(
case when issotrx = 'Y'
	and (date(col.datepromised)-date(now())) > 15 and (date(col.datepromised)-date(now())) <= 30
	then qtyordered
	else 0
end
) as porentregar_1530,
(
case when issotrx = 'Y'
	and (date(col.datepromised)-date(now())) > 30 and (date(col.datepromised)-date(now())) <= 60
	then qtyordered
	else 0
end
) as porentregar_3060,
(
case when issotrx = 'Y'
	and (date(col.datepromised)-date(now())) > 60
	then qtyordered
	else 0
end
) as porentregar_60,
(
case when issotrx = 'N'
	and (date(col.datepromised)-date(now())) <= 15
	then qtyordered
	else 0
end
) as porrecibir_0015,
(
case when issotrx = 'N'
	and (date(col.datepromised)-date(now())) > 15 and (date(col.datepromised)-date(now())) <= 30
	then qtyordered
	else 0
end
) as porrecibir_1530,
(
case when issotrx = 'N'
	and (date(col.datepromised)-date(now())) > 30 and (date(col.datepromised)-date(now())) <= 60
	then qtyordered
	else 0
end
) as porrecibir_3060,
(
case when issotrx = 'N'
	and (date(col.datepromised)-date(now())) > 60
	then qtyordered
	else 0
end
) as porrecibir_60


from c_orderline col
left join c_order co on co.c_order_id = col.c_order_id
left join m_inoutline iol on iol.c_orderline_id = col.c_orderline_id
where iol.c_orderline_id IS NULL
and co.docstatus != 'CL' and co.docstatus != 'DR' and co.docstatus != 'IN'  and co.docstatus != 'VO'
and co.c_bpartner_id != 1012821 and col.m_product_id is not null

