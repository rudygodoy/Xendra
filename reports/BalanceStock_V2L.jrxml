	else 0
end
) as porxxx_2,
(
case when min(issotrx) = 'Y' and max(oqtyenterd)-sum(qtyentered) >= 0 and min(docstatus) != 'CL'
	then max(oqtyenterd)-sum(qtyentered)
	else 0
end
