  <queryString><![CDATA[select
'X'::CHAR as p_status,
'1990-01-01'::TIMESTAMP WITHOUT TIME ZONE as fecTrans,
''::TEXT as pagdocno,
'' as libro ,
''::VARCHAR as voucherno,
''::VARCHAR(40) as codCuenta,
''::VARCHAR(60)  as tipoDoc,
'' as docMov,
'' as description,
'' as moneda,
0 as egreso,
0 as ingreso,
xx.cbancaria,
(CASE WHEN xx.ingresoDolar-xx.egresoDolar > 0
	then xx.ingresoDolar-xx.egresoDolar
	else null
end) as ingresoDolar,
(CASE WHEN xx.ingresoDolar-xx.egresoDolar < 0
	then xx.egresoDolar-xx.ingresoDolar
	else null
end) as egresoDolar,
(CASE WHEN xx.ingresoEuro-xx.egresoEuro > 0
	then xx.ingresoEuro-xx.egresoEuro
	else 0
end) as ingresoEuro,
(CASE WHEN xx.ingresoEuro-xx.egresoEuro < 0
	then xx.egresoEuro-xx.ingresoEuro
	else 0
end) as egresoEuro,
(CASE WHEN xx.ingresoSoles-xx.egresoSoles > 0
	then xx.ingresoSoles-xx.egresoSoles
	else 0
end) as ingresoSoles,
(CASE WHEN xx.ingresoSoles-xx.egresoSoles < 0
	then xx.egresoSoles-xx.ingresoSoles
	else 0
end) as egresoSoles,
(CASE WHEN xx.debito-xx.credito > 0
	then xx.debito-xx.credito
	else 0
end) as debito,
(CASE WHEN xx.debito-xx.credito < 0
	then xx.credito-xx.debito
	else 0
end) as credito,
--xx.debito,
--xx.credito,
xx.payment_id,
xx.noContab,
pe.startdate,
pe.enddate
from (
select
bn.name || ' - ' || ba.accountno as cbancaria,
sum(
case when cur.cursymbol = '$'
then
	case when p.isreceipt = 'Y' or (coalesce(p.isreceipt,'N') = 'N' and p.c_doctype_id = 1001058 and coalesce(p.dest_payment_id,0) = 0)
		then p.payamt
		else 0
	end
else 0
end
) as ingresoDolar,
sum(
case when cur.cursymbol = '$'
then
	case when (coalesce(p.isreceipt,'N') = 'N' and p.c_doctype_id != 1001058) or
			( coalesce(p.isreceipt,'N') = 'N' and (p.c_doctype_id = 1001058 and coalesce(p.dest_payment_id,0) != 0))
		then p.payamt
		else 0
	end
	else 0
end
) as egresoDolar,
sum(
case when cur.cursymbol = 'E'
then
	case when p.isreceipt = 'Y' or (coalesce(p.isreceipt,'N') = 'N' and p.c_doctype_id = 1001058 and coalesce(p.dest_payment_id,0) = 0)
		then p.payamt
		else 0
	end
else 0
end
) as ingresoEuro,
sum(
case when cur.cursymbol = 'E'
then
	case when (coalesce(p.isreceipt,'N') = 'N' and p.c_doctype_id != 1001058) or
			( coalesce(p.isreceipt,'N') = 'N' and (p.c_doctype_id = 1001058 and coalesce(p.dest_payment_id,0) != 0))
		then p.payamt
		else 0
	end
	else 0
end
) as egresoEuro,
sum(
case when cur.cursymbol = 'S/.'
then
	case when p.isreceipt = 'Y' or (coalesce(p.isreceipt,'N') = 'N' and p.c_doctype_id = 1001058 and coalesce(p.dest_payment_id,0) = 0)
		then p.payamt
		else 0
	end
else 0
end
) as ingresoSoles,
sum(
case when cur.cursymbol = 'S/.'
then
	case when (coalesce(p.isreceipt,'N') = 'N' and p.c_doctype_id != 1001058) or
			( coalesce(p.isreceipt,'N') = 'N' and (p.c_doctype_id = 1001058 and coalesce(p.dest_payment_id,0) != 0))
		then p.payamt
		else 0
	end
	else 0
end
) as egresoSoles,
sum (case when coalesce(fa.fact_id,'') != ''
	then fa.amtacctdr
     when coalesce(fa2.fact_id,'')!=''
 	then fa2.amtacctdr
 else
     null
 end
) as debito,
sum (case when coalesce(fa.fact_id,'') != ''
	then fa.amtacctcr
     when coalesce(fa2.fact_id,'')!=''
 	then fa2.amtacctcr
 else
     null
 end
) as credito,
0 as payment_id,
-- Cuenta los no contabilizados
sum(
(case when coalesce(fa.fact_acct_id,0)=0 and coalesce(fa2.fact_acct_id,0)=0
	then 1
	else 0
end)
) as noContab
from c_payment p
left join fact_acct fa on fa.fact_acct_id = (select min(ff.fact_acct_id) from fact_acct ff where ff.ad_table_id = (select ad_table_id from ad_table a where upper(tablename)='C_ALLOCATIONHDR')
	and record_id = (select c_allocationhdr_id from c_allocationline where c_payment_id=p.c_payment_id limit 1) group by ff.record_id)
left join fact_acct fa2 on fa2.fact_acct_id = (select min(ff.fact_acct_id) from fact_acct ff where ff.ad_table_id = (select ad_table_id from ad_table a where upper(tablename)='C_PAYMENT') and (ff.record_id = p.c_payment_id
	or ff.record_id = p.dest_payment_id) group by ff.record_id)
left join c_bankaccount ba on ba.c_bankaccount_id = p.c_bankaccount_id
left join c_bank bn on bn.c_bank_id = ba.c_bank_id
left join c_currency cur on cur.c_currency_id = p.c_currency_id
left join c_period pe on pe.c_period_id =  $P{C_Period_ID}
where p.isactive='Y' and ba.c_bankaccount_id != 1000058 and  ba.c_bankaccount_id != 1000059
	and p.ad_client_id = 1000026 and p.ad_org_id=1000069
	and p.datetrx < pe.startdate
	and (case when coalesce($P{C_BankAccount_ID},0) = 0
		then true
		else p.c_bankaccount_id = $P{C_BankAccount_ID}
		end)
group by cbancaria,payment_id
) xx
left join c_period pe on pe.c_period_id = $P{C_Period_ID}

union
(
select
(
case when p.docstatus != 'CO' and p.docstatus != 'CL'
	then 'B'
	else null
end
) as p_status,
