	AND (CASE WHEN COALESCE($P{AD_Org_ID}, 0)  = 0 THEN true ELSE f.ad_org_id = $P{AD_Org_ID} END)
	AND f.c_acctschema_id = $P{C_AcctSchema_ID}
	AND f.postingtype = $P{PostingType}
	AND COALESCE(f.isclosedocument,'N') = 'N'
	AND (CASE WHEN COALESCE($P{DateAcct}, p.enddate) = p.enddate THEN true
		ELSE
	     f.dateacct <= $P{DateAcct} END)
	group by sub.value
) as factline
) as fact,tax,client,schema]]></queryString>
