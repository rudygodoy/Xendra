  <queryString><![CDATA[-- nuevo formato 3.17A
with fiscalyear as ( select (select fiscalyear from c_year y where y.c_year_id = p.c_year_id)  as fyear from c_period p where c_period_id = $P{C_Period_ID} ),
prevperiod as (SELECT c_period_id, name,enddate,periodno FROM c_period WHERE periodno < (SELECT periodno FROM c_period WHERE c_period_id = $P{C_Period_ID} ) AND c_year_id = (SELECT c_year_id FROM c_period WHERE c_period_id = $P{C_Period_ID} )),
currperiod as (SELECT c_period_id, name,enddate,periodno FROM c_period WHERE c_period_id = $P{C_Period_ID} ),
	tax as (SELECT CASE WHEN $P{AD_Org_ID} =0 THEN max(ci.taxid) ELSE max(oi.taxid) END as id FROM ad_orginfo oi
		JOIN ad_clientinfo ci on ci.ad_client_id = oi.ad_client_id WHERE ci.ad_client_id = $P{AD_Client_ID}),
period as (SELECT c_period_id, name,enddate,periodno FROM c_period WHERE periodno <= (SELECT periodno FROM c_period WHERE c_period_id = $P{C_Period_ID} ) AND c_year_id = (SELECT c_year_id FROM c_period WHERE c_period_id = $P{C_Period_ID} )),
     client as (SELECT CASE WHEN $P{AD_Org_ID}=0 THEN (SELECT description FROM AD_Client where AD_Client_ID = $P{AD_Client_ID})
				     ELSE (SELECT description FROM AD_org where AD_Org_ID = $P{AD_Org_ID}) END as description),
     schema as (SELECT acc.c_acctschema_id, acc.name FROM c_acctschema acc WHERE acc.c_acctschema_id = $P{C_AcctSchema_ID})
SELECT
	foo.cliente,
	foo.ruc,
	foo.periodo,
	foo.esquema,
	foo.cuenta,
	foo.descripcion,
	SUM(foo.debe_i) as debe_i,
	SUM(foo.haber_i) as haber_i,
	SUM(foo.debe_m) as debe_m,
	SUM(foo.haber_m) as haber_m,
	SUM(foo.debe_f) as debe_f,
	SUM(foo.haber_f) as haber_f,
	SUM(foo.activo_b) as activo_b,
	SUM(foo.pasivo_b) as pasivo_b,
	SUM(foo.gasto_rn) as gasto_rn,
	SUM(foo.ingreso_rn) as ingreso_rn,
	SUM(foo.gasto_rf) as gasto_rf,
	SUM(foo.ingreso_rf) as ingreso_rf
FROM(
	-- saldo inicial
	SELECT
	client.description as cliente,
	tax.id  as ruc,
	(SELECT p.name FROM c_period p WHERE p.c_period_id = $P{C_Period_ID} ) as periodo,
	acc.name as esquema,
	(left(ev.value,4)) as cuenta,
	(select name from c_elementvalue where (left(ev.value,4)) = value and AD_Client_ID = $P{AD_Client_ID}) as descripcion,
	SUM(f.amtacctdr) as debe_i,
	SUM(f.amtacctcr) as haber_i,
	0 as debe_m,
	0 as haber_m,
	0 as debe_f,
	0 as haber_f,
	0 as activo_b,
	0 as pasivo_b,
	0 as gasto_rn,
	0 as ingreso_rn,
	0 as gasto_rf,
	0 as ingreso_rf
	FROM fact_acct f
	JOIN schema acc on f.c_acctschema_id = acc.c_acctschema_id
	JOIN c_elementvalue ev on f.account_id = ev.c_elementvalue_id, tax, client, fiscalyear
	WHERE f.ad_client_id = $P{AD_Client_ID}
	AND (CASE WHEN COALESCE($P{AD_Org_ID}, 0)  = 0
	       THEN true
	       ELSE f.ad_org_id = $P{AD_Org_ID} END)
	AND f.isopendocument = 'Y'
	AND f.postingtype = $P{PostingType}
	AND extract(year from dateacct) = fiscalyear.fyear::integer
	GROUP BY tax.id, client.description, acc.name, ev.value, ev.name
	UNION all
	-- movimiento
	SELECT
	client.description as cliente,
	tax.id  as ruc,
	(SELECT p.name FROM c_period p WHERE p.c_period_id = $P{C_Period_ID}) as periodo,
	acc.name as esquema,
	(left(ev.value,4)) as cuenta,
	(select name from c_elementvalue where (left(ev.value,4)) = value and AD_Client_ID = $P{AD_Client_ID}) as descripcion,
	0 as debe_i,
	0 as haber_i,
	SUM(f.amtacctdr) as debe_m,
	SUM(f.amtacctcr) as haber_m,
	0 as debe_f,
	0 as haber_f,
	0 as activo_b,
	0 as pasivo_b,
	0 as gasto_rn,
	0 as ingreso_rn,
	0 as gasto_rf,
	0 as ingreso_rf
	FROM fact_acct f
	JOIN period p on f.c_period_id = p.c_period_id
	JOIN schema acc on f.c_acctschema_id = acc.c_acctschema_id
	JOIN c_elementvalue ev on f.account_id = ev.c_elementvalue_id, tax, client, fiscalyear
	WHERE f.ad_client_id = $P{AD_Client_ID}
	AND COALESCE(f.isopendocument,'N') = 'N'
	AND COALESCE(f.isclosedocument,'N') = 'N'
	AND f.postingtype = $P{PostingType}
	AND extract(year from dateacct) = fiscalyear.fyear::integer
	AND (CASE WHEN COALESCE($P{AD_Org_ID}, 0)  = 0
	       THEN true
	       ELSE f.ad_org_id = $P{AD_Org_ID} END)
	GROUP BY tax.id, client.description, acc.name, ev.value, ev.name

	UNION all
	-- saldo final
	SELECT
	client.description as cliente,
	tax.id  as ruc,
	(SELECT p.name FROM c_period p WHERE p.c_period_id = $P{C_Period_ID}) as periodo,
	acc.name as esquema,
	(left(ev.value,4)) as cuenta,
	(select name from c_elementvalue where (left(ev.value,4)) = value and AD_Client_ID = $P{AD_Client_ID}) as descripcion,
	0 as debe_i,
	0 as haber_i ,
	0 as debe_m,
	0 as haber_m,
	SUM(f.amtacctdr) as debe_f,
	SUM(f.amtacctcr) as haber_f,
	0 as activo_b,
	0 as pasivo_b,
	0 as gasto_rn,
	0 as ingreso_rn,
	0 as gasto_rf,
	0 as ingreso_rf
	FROM fact_acct f
	join period p on f.c_period_id = p.c_period_id
	JOIN schema acc on f.c_acctschema_id = acc.c_acctschema_id
	JOIN c_elementvalue ev on f.account_id = ev.c_elementvalue_id, tax, client
	WHERE f.ad_client_id = $P{AD_Client_ID}
	AND (CASE WHEN COALESCE($P{AD_Org_ID}, 0)  = 0
	       THEN true
	       ELSE f.ad_org_id = $P{AD_Org_ID} END)
	AND f.postingtype = $P{PostingType}
	AND COALESCE(f.isclosedocument,'N') = 'N'
	GROUP BY tax.id, client.description, acc.name, ev.value, ev.name
	UNION all
	-- balance
	SELECT
	client.description as cliente,
	tax.id  as ruc,
	(SELECT p.name FROM c_period p WHERE p.c_period_id = $P{C_Period_ID}) as periodo,
	acc.name as esquema,
	(left(ev.value,4)) as cuenta,
	(select name from c_elementvalue where (left(ev.value,4)) = value and AD_Client_ID = $P{AD_Client_ID}) as descripcion,
	0 as debe_i,
	0 as haber_i ,
	0 as debe_m,
	0 as haber_m,
	0 as debe_f,
	0 as haber_f,
	SUM(f.amtacctdr) as activo_b,
	SUM(f.amtacctcr) as pasivo_b,
	0 as gasto_rn,
	0 as ingreso_rn,
	0 as gasto_rf,
	0 as ingreso_rf
	FROM fact_acct f
	JOIN ad_org o on f.ad_org_id = o.ad_org_id
	JOIN ad_orginfo oi on o.ad_org_id = oi.ad_org_id
	JOIN period p on f.c_period_id = p.c_period_id
	JOIN schema acc on f.c_acctschema_id = acc.c_acctschema_id
	JOIN c_elementvalue ev on f.account_id = ev.c_elementvalue_id, tax, client
	WHERE f.ad_client_id = $P{AD_Client_ID}
	AND (CASE WHEN COALESCE($P{AD_Org_ID}, 0)  = 0
	       THEN true
	       ELSE f.ad_org_id = $P{AD_Org_ID} END)
	AND f.postingtype = $P{PostingType}
	AND COALESCE(f.isclosedocument,'N') = 'N'
	AND ev.c_subject_id > 0
	GROUP BY tax.id, client.description, acc.name, ev.value, ev.name

	UNION all
	-- resultados naturaleza
	SELECT
	client.description as cliente,
	tax.id  as ruc,
	(SELECT p.name FROM c_period p WHERE p.c_period_id = $P{C_Period_ID}) as periodo,
	acc.name as esquema,
	(left(ev.value,4)) as cuenta,
	(select name from c_elementvalue where (left(ev.value,4)) = value and AD_Client_ID = $P{AD_Client_ID}) as descripcion,
	0 as debe_i,
	0 as haber_i ,
	0 as debe_m,
	0 as haber_m,
	0 as debe_f,
	0 as haber_f,
	0 as activo_b,
	0 as pasivo_b,
	SUM(f.amtacctdr) as gasto_rn,
	SUM(f.amtacctcr) as ingreso_rn,
	0 as gasto_rf,
	0 as ingreso_rf
	FROM fact_acct f
	join period p on f.c_period_id = p.c_period_id
	JOIN schema acc on f.c_acctschema_id = acc.c_acctschema_id
	JOIN c_elementvalue ev on f.account_id = ev.c_elementvalue_id, tax, client
	WHERE f.ad_client_id = $P{AD_Client_ID}
	AND (CASE WHEN COALESCE($P{AD_Org_ID}, 0)  = 0
	       THEN true
	       ELSE f.ad_org_id = $P{AD_Org_ID} END)
	AND f.postingtype = $P{PostingType}
	AND COALESCE(f.isclosedocument,'N') = 'N'
	AND ev.egpn_c_subject_id > 0
	GROUP BY tax.id, client.description, acc.name, ev.value, ev.name

	-- resultados funcion
	UNION all
	SELECT
	client.description as cliente,
	tax.id  as ruc,
	(SELECT p.name FROM c_period p WHERE p.c_period_id = $P{C_Period_ID} ) as periodo,
	acc.name as esquema,
	(left(ev.value,4)) as cuenta,
	(select name from c_elementvalue where (left(ev.value,4)) = value and AD_Client_ID = $P{AD_Client_ID}) as descripcion,
	0 as debe_i,
	0 as haber_i ,
	0 as debe_m,
	0 as haber_m,
	0 as debe_f,
	0 as haber_f,
	0 as activo_b,
	0 as pasivo_b,
	0 as gasto_rn,
	0 as ingreso_rn,
	SUM(f.amtacctdr) as gasto_rf,
	SUM(f.amtacctcr) as ingreso_rf
	FROM fact_acct f
	JOIN period p on f.c_period_id = p.c_period_id
	JOIN schema acc on f.c_acctschema_id = acc.c_acctschema_id
	JOIN c_elementvalue ev on f.account_id = ev.c_elementvalue_id, client, tax
	WHERE f.ad_client_id = $P{AD_Client_ID}
	AND (CASE WHEN COALESCE($P{AD_Org_ID}, 0)  = 0
	       THEN true
	       ELSE f.ad_org_id =  $P{AD_Org_ID} END)
	AND f.postingtype = $P{PostingType}
	AND COALESCE(f.isclosedocument,'N') = 'N'
	AND ev.egpf_c_subject_id > 0
	GROUP BY tax.id, client.description, acc.name, ev.value, ev.name) as foo
	GROUP BY foo.cliente, foo.ruc, foo.periodo, foo.esquema, foo.cuenta, foo.descripcion
	ORDER BY foo.cuenta]]></queryString>
