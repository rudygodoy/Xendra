fact_acct f
	JOIN prevperiod p on f.c_period_id = p.c_period_id
	JOIN c_elementvalue ev on f.account_id= ev.c_elementvalue_id
	JOIN c_subject sub on ev.c_subject_id = sub.c_subject_id
	WHERE f.ad_client_id = $P{AD_Client_ID}
	AND (CASE WHEN COALESCE($P{AD_Org_ID}, 0)  = 0 THEN true ELSE f.ad_org_id = $P{AD_Org_ID} END)
	AND f.c_acctschema_id = $P{AD_Client_ID}
	AND f.postingtype = $P{PostingType}
	AND COALESCE(f.isclosedocument,'N') = 'N'
	AND (CASE WHEN COALESCE('2012/12/31'::timestamp, p.enddate) = p.enddate THEN true
		ELSE
	     f.dateacct <= '2012/12/31'::timestamp END)
	group by sub.value
