	left join c_elementvalue ev on f.account_id = ev.c_elementvalue_id
--	left join c_period p on f.c_period_id = p.c_period_id
	where
	f.ad_client_id = $P{AD_Client_ID}
and f.ad_org_id = $P{AD_Org_ID}
and
	f.dateacct >= date(EXTRACT(YEAR FROM TIMESTAMP '$P!{DateFrom1}') || '-01-01') and f.dateacct <= $P{DateFrom2}
