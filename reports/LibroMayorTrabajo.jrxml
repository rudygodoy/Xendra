  <queryString><![CDATA[-------------------------------------------------------------------------
---- SALDOS INICIALES
-------------------------------------------------------------------------
-- primero totalizamos los movimientos a nivel de digitacion


select
--(select name from c_period where c_period_id=1000452) as periodo,
'a' as tipo,
(select codsunat from gl_category ct where ct.gl_category_id = $P{GL_Category_ID}) as libro,
'2000-01-01 00:00:00'::timestamp without time zone as fecha_contable,
'2000-01-01 00:00:00'::timestamp without time zone as fecha_operacion,
'' as moneda,
'' as nombreSocio,
'' as voucher,
'' as descripcion,
'' as docnro,
'' as doctipo,
'' as observacion,
ev.value as cuenta,
ev.name as ncuenta,
sum(amtacctdr) as debe,
sum(amtacctcr) as haber,
sum(amtacctdr)-sum(amtacctcr) as saldo,
sum(case when f.c_currency_id = 102
	then amtsourcedr
	else 0
	end
) as debe_euro,
sum(case when f.c_currency_id = 102
	then amtsourcecr
	else 0
	end
) as haber_euro,
sum(case when f.c_currency_id = 100
	then amtsourcedr
	else 0
	end
) as debe_dolar,
sum(case when f.c_currency_id = 100
	then amtsourcecr
	else 0
	end
) as haber_dolar,
ev.value as cuenta_porden

from fact_acct f
left join c_elementvalue ev on f.account_id = ev.c_elementvalue_id
--left join c_period p on f.c_period_id = p.c_period_id

where
f.ad_client_id = $P{AD_Client_ID}
and f.ad_org_id = $P{AD_Org_ID}
and
f.dateacct >= date(EXTRACT(YEAR FROM TIMESTAMP '$P!{DateFrom1}') || '-01-01') and f.dateacct < $P{DateFrom1}
and
(case when coalesce($P{P_Cuenta},'') != ''
   then substr(ev.value,1,length($P{P_Cuenta})) = $P{P_Cuenta}
	else true
end)
and
(case when coalesce($P{GL_Category_ID},0) != 0
   then f.gl_category_id = $P{GL_Category_ID}
	else true
end)
and
(case when coalesce($P{C_BPartner_ID},0) != 0
   then f.c_bpartner_id = $P{C_BPartner_ID}
	else true
end)
group by ev.value,ev.name

union
-- Acumulamos a las cuentas de menos de 8 digitos
select
'a' as tipo,
(select codsunat from gl_category ct where ct.gl_category_id = $P{GL_Category_ID}) as libro,
'2000-01-01 00:00:00'::timestamp without time zone as fecha_contable,
'2000-01-01 00:00:00'::timestamp without time zone as fecha_operacion,
'' as moneda,
'' as nombreSocio,
'' as voucher,
'' as descripcion,
'' as docnro,
'' as doctipo,
'' as observacion,
e.value as cuenta,
e.name  as ncuenta,
sum(amtacctdr) as debe,
sum(amtacctcr) as haber,
sum(amtacctdr)-sum(amtacctcr) as saldo,
sum(debe_euro) as debe_euro,
sum(haber_euro) AS haber_euro,
sum(debe_dolar) AS debe_dolar,
sum(haber_dolar) AS haber_dolar,
e.value as cuenta_porden

from c_elementvalue e
join (
	select f.account_id,ev.value,sum(amtacctdr) as amtacctdr ,sum(amtacctcr) as amtacctcr ,
		sum(case when f.c_currency_id = 102
			then amtsourcedr
			else 0
			end
		) as debe_euro,
		sum(case when f.c_currency_id = 102
			then amtsourcecr
			else 0
			end
		) as haber_euro,
		sum(case when f.c_currency_id = 100
			then amtsourcedr
			else 0
			end
		) as debe_dolar,
		sum(case when f.c_currency_id = 100
			then amtsourcecr
			else 0
			end
		) as haber_dolar
	from fact_acct f
	left join c_elementvalue ev on f.account_id = ev.c_elementvalue_id
--	left join c_period p on f.c_period_id = p.c_period_id
	where
	f.ad_client_id = $P{AD_Client_ID}
and f.ad_org_id = $P{AD_Org_ID}
and
	f.dateacct >= date(EXTRACT(YEAR FROM TIMESTAMP '$P!{DateFrom1}') || '-01-01') and f.dateacct < $P{DateFrom1}
	and
	(case when coalesce($P{P_Cuenta},'') != ''
   		then substr(ev.value,1,length($P{P_Cuenta})) = $P{P_Cuenta}
		else true
	end)
	and
	(case when coalesce($P{GL_Category_ID},0) != 0
   		then f.gl_category_id = $P{GL_Category_ID}
		else true
	end)
	and
	(case when coalesce($P{C_BPartner_ID},0) != 0
   		then f.c_bpartner_id = $P{C_BPartner_ID}
		else true
	end)

	group by f.account_id,ev.value

) f on e.value = substr((select e2.value from c_elementvalue e2 where e2.c_elementvalue_id = f.account_id),1,length(e.value))

where length(e.value) < 8

group by e.value,e.name

-- Ls detalles del movimiento del mes
union
select
'd' as tipo,
ct.codsunat as libro,
f.dateacct as fecha_contable,
f.datetrx as fecha_operacion,
cy.codsunat as moneda,
bp.name as nombreSocio,
f.fact_id as voucher,
f.description as descripcion,
-- PARCHE PARI PARA OBTENER EL NUMERO DE LETRA DDESDE UNA ASIGNACION
-- ES Asignacion y tiene una linea especificada ?
(case when f.ad_table_id=735 and f.line_id is not null then
	(
	  -- Si la linea de asignacion se refiere a la tabla c_boe
	  case when (select ad_table_id from c_allocationline where c_allocationline_id = f.line_id and c_allocationhdr_id = f.record_id) = 1000065 then
		(select documentno from c_boe where c_boe_id = (select record_id from c_allocationline al2 where al2.c_allocationline_id = f.line_id and al2.c_allocationhdr_id = f.record_id))
		-- Si la linea de asignacion tiene un boe_id indicado
		when (select c_boe_id from c_allocationline where c_allocationline_id = f.line_id and c_allocationhdr_id = f.record_id) is not null then
		 (select documentno from c_boe b where c_boe_id =
			(select c_boe_id from c_allocationline where c_allocationline_id = f.line_id and c_allocationhdr_id = f.record_id))
	  else
	   (select docnro from getdocumentnodatafromrecordid(f.ad_client_id, f.ad_org_id, t.tablename, f.record_id))
	  end
	)
	else
	 (select docnro from getdocumentnodatafromrecordid(f.ad_client_id, f.ad_org_id, t.tablename, f.record_id))
	end
) as docnro,
-- PARCHE PARI PARA OBTENER EL NUMERO DE LETRA DDESDE UNA ASIGNACION
-- ES Asignacion y tiene una linea especificada ?
(case when f.ad_table_id=735 and f.line_id is not null then
	(
	  -- Si la linea de asignacion se refiere a la tabla c_boe
	  case when (select ad_table_id from c_allocationline where c_allocationline_id = f.line_id and c_allocationhdr_id = f.record_id) = 1000065 then
		     (select (select name from c_doctype_trl dt where dt.c_doctype_id = i.c_doctype_id) from c_boe i
				where c_boe_id = (select record_id from c_allocationline al2 where al2.c_allocationline_id = f.line_id and al2.c_allocationhdr_id = f.record_id))
		-- Si la linea de asignacion tiene un boe_id indicado
		when (select c_boe_id from c_allocationline where c_allocationline_id = f.line_id and c_allocationhdr_id = f.record_id) is not null then
		     (select (select name from c_doctype_trl dt where dt.c_doctype_id = i.c_doctype_id) from c_boe i
				where c_boe_id = (select c_boe_id from c_allocationline where c_allocationline_id = f.line_id and c_allocationhdr_id = f.record_id))
		else
		     (select docname from getdocumentnodatafromrecordid(f.ad_client_id, f.ad_org_id, t.tablename, f.record_id))
	  end
	)
	else
	 (select docname from getdocumentnodatafromrecordid(f.ad_client_id, f.ad_org_id, t.tablename, f.record_id))
	end
) as doctipo,
--(select docnro from getdocumentnodatafromrecordid(f.ad_client_id, f.ad_org_id, t.tablename, f.record_id)) as docnro,
--(select docname from getdocumentnodatafromrecordid(f.ad_client_id, f.ad_org_id, t.tablename, f.record_id)) as doctipo,
'' as observacion,
ev.value as cuenta,
ev.name  as ncuenta,
amtacctdr as debe,
amtacctcr as haber,
amtacctdr-amtacctcr as saldo,
(case when f.c_currency_id = 102
	then amtsourcedr
	else 0
	end
) as debe_euro,
(case when f.c_currency_id = 102
	then amtsourcecr
	else 0
	end
) as haber_euro,
(case when f.c_currency_id = 100
	then amtsourcedr
	else 0
	end
) as debe_dolar,
(case when f.c_currency_id = 100
	then amtsourcecr
	else 0
	end
) as haber_dolar,
ev.value as cuenta_porden
from fact_acct f
left join c_elementvalue ev on f.account_id = ev.c_elementvalue_id
--left join c_period p on f.c_period_id = p.c_period_id
left join gl_category ct on f.gl_category_id = ct.gl_category_id
left join c_currency cy on f.c_currency_id=cy.c_currency_id
left join c_bpartner bp on bp.c_bpartner_id = f.c_bpartner_id
left join ad_table t on t.ad_table_id = f.ad_table_id

where
f.ad_client_id = $P{AD_Client_ID}

and f.ad_org_id = $P{AD_Org_ID}
and f.dateacct between $P{DateFrom1} and $P{DateFrom2}
and
(case when coalesce($P{P_Cuenta},'') != ''
   then substr(ev.value,1,length($P{P_Cuenta})) = $P{P_Cuenta}
	else true
end)
and
(case when coalesce($P{GL_Category_ID},0) != 0
   then f.gl_category_id = $P{GL_Category_ID}
	else true
end)
and
(case when coalesce($P{C_BPartner_ID},0) != 0
   then f.c_bpartner_id = $P{C_BPartner_ID}
	else true
end)


