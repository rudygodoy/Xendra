  <queryString><![CDATA[select m_product_group."name" AS m_product_group_name,
m_product_type."name" AS m_product_type_name,
m_product_brand."name" AS m_product_brand_name,
m_product_generic."name" AS m_product_generic_name,
prod.value,
prod.name,
prod_totalizado.* from (
	------------------------------------------
	-- AGRUPADO FINAL - ACUMULADOR POR PRODUCTO AÑO,MES
	--------------------------------------------------
	select codproducto,ano,
	sum(enero) as enero,
	sum(febrero) as febrero,
	sum(marzo) as marzo,
	sum(abril) as abril,
	sum(mayo) as mayo,
	sum(junio) as junio,
	sum(julio) as julio,
	sum(agosto) as agosto,
	sum(setiembre) as setiembre,
	sum(octubre) as octubre,
	sum(noviembre) as noviembre,
	sum(diciembre) as diciembre,
	sum(enero)+sum(febrero)+sum(marzo)+sum(abril)+sum(mayo)+sum(junio)+sum(julio)+sum(agosto)+sum(setiembre)+
	sum(octubre)+sum(noviembre)+sum(diciembre) as totalamo


	from (
	------------------------------------
	-- Se separa por meses
	------------------------------------
	select m_product_id as codproducto,ano,

		case when mes = 1
			then totunidades
			else 0
		end as Enero,
		case when mes = 2
			then totunidades
			else 0
		end as Febrero,
		case when mes = 3
			then totunidades
			else 0
		end as Marzo,
		case when mes = 4
			then totunidades
			else 0
		end as Abril,
		case when mes = 5
			then totunidades
			else 0
		end as Mayo,
		case when mes = 6
			then totunidades
			else 0
		end as Junio,
		case when mes = 7
			then totunidades
			else 0
		end as Julio,
		case when mes = 8
			then totunidades
			else 0
		end as Agosto,
		case when mes = 9
			then totunidades
			else 0
		end as Setiembre,
		case when mes = 10
			then totunidades
			else 0
		end as Octubre,
		case when mes = 11
			then totunidades
			else 0
		end as Noviembre,
		case when mes = 12
			then totunidades
			else 0
		end as Diciembre


		 from (
			---------------------
			-- Primero se acumula agrupado por product , mes y año
			------------------------------
			select
			--inv.c_doctype_id,
			--dt.name,
			il.m_product_id,
			--p.value,
			--p.name as producto,
			extract(year from dateinvoiced) as ano,
			extract(month from dateinvoiced) as mes,
			sum(qtyinvoiced) totunidades

			from c_invoiceline il
			join c_invoice inv on inv.c_invoice_id = il.c_invoice_id
			join m_product p on p.m_product_id = il.m_product_id
			join c_doctype_trl dt on dt.c_doctype_id=inv.c_doctype_id
			where
			issotrx  ='Y' 
			and inv.docstatus != 'DR' 
			and inv.docstatus != 'IN'  
			and inv.docstatus != 'VO' 
			and inv.isactive='Y' 
			and inv.ad_client_id = $P{AD_Client_ID} 
			and inv.ad_org_id= $P{AD_Org_ID} 
			-- No a las transferencias gratuitas
			and inv.c_doctype_id != 1001127
			-- No alas notas de ningun tipo , las devoluciones seran sacadas de las guias
			and inv.c_doctype_id != 1001042 
			and inv.c_doctype_id != 1001092
			and extract(year from dateinvoiced) = $P{P_Anno}
			and inv.c_bpartner_id != 1012821
			group by il.m_product_id,ano,mes

			union all
			-- Las devoluciones las hacemos toamndo en cuenta las guias
			-- por ser mas preciso , a veces las notas no indican con precision
			-- el numero de unidades.
			select il.m_product_id,
			extract(year from movementdate) as ano,
			extract(month from movementdate) as mes,
			-1*sum(il.qtyentered) totunidades
			from m_inout io
			left join m_inoutline il on il.m_inout_id=io.m_inout_id and il.isactive  = 'Y'
			left join c_orderline ol on ol.c_orderline_id = il.c_orderline_id
			left join c_order ord on ord.c_order_id = ol.c_order_id
			left join c_doctype_trl dt on dt.c_doctype_id = ord.c_doctype_id
			where io.isactive  = 'Y' 
			and io.docstatus != 'DR'
			and io.docstatus != 'IN'  
			and io.docstatus != 'VO' --and io.issotrx = 'Y'
			and io.ad_client_id =  $P{AD_Client_ID}
			and io.ad_org_id=  $P{AD_Org_ID}
			and
			-- Solo por devoluciones que provienen de una venta
			-- Si c_doctype_id = 1001110 es una devolucion sin N/C solo implica
			-- devolucion real de mercaderia si apunta a una factura.
			((ord.c_doctype_id = 1001110 and not ord.ref_invoice_id is null) or
			ord.c_doctype_id = 1001111 or
			ord.c_doctype_id = 1001109 or
			ord.c_doctype_id = 1001066)
			and extract(year from movementdate) = $P{P_Anno}
			and io.c_bpartner_id != 1012821
			group by il.m_product_id,ano,mes


		) total_ano_mes
	) acumulador_final
	group by codproducto,ano
	order by codproducto,ano
) prod_totalizado
    left join m_product prod on codproducto = prod.m_product_id
     left join "m_product_group" m_product_group ON prod."m_product_group_id" = m_product_group."m_product_group_id"
     left join "m_product_brand" m_product_brand ON prod."m_product_brand_id" = m_product_brand."m_product_brand_id"
     left join "m_product_type" m_product_type ON prod."m_product_type_id" = m_product_type."m_product_type_id"
     left join "m_product_generic" m_product_generic ON prod."m_product_generic_id" = m_product_generic."m_product_generic_id"
	where prod.ad_client_id = $P{AD_Client_ID} and prod.isactive = 'Y' and prod.issummary ='N'
    AND
     (CASE WHEN COALESCE($P{M_Product_Group_ID}, 0)  = 0
        THEN true
        ELSE prod.m_product_group_id = $P{M_Product_Group_ID}
    END)
    AND
     (CASE WHEN COALESCE($P{M_Product_Type_ID}, 0)  = 0
        THEN true
        ELSE prod.m_product_type_id = $P{M_Product_Type_ID}
    END)
    AND
     (CASE WHEN COALESCE($P{M_Product_Brand_ID}, 0)  = 0
        THEN true
        ELSE prod.m_product_brand_id = $P{M_Product_Brand_ID}
    END)
    AND
     (CASE WHEN COALESCE($P{M_Product_Generic_ID}, 0)  = 0
        THEN true
        ELSE prod.m_product_generic_id = $P{M_Product_Generic_ID}
    END)
order by
m_product_group."name",
m_product_type."name",
m_product_brand."name",
m_product_generic."name",
prod.name]]></queryString>
