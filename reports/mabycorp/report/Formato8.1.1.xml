    <queryString><![CDATA[SELECT
i.fact_id as correlativo,
(select name from ad_client where ad_client_id = i.ad_client_id) AS name,

(SELECT CASE WHEN $P{AD_Org_ID}=0
	THEN
	(SELECT taxid FROM ad_clientinfo where ad_client_id = $P{AD_Client_ID})
	ELSE
	(SELECT taxid FROM ad_orginfo WHERE ad_client_id = $P{AD_Client_ID} and ad_org_id = $P{AD_Org_ID})
END) AS ruc,
(SELECT CASE WHEN $P{AD_Org_ID}=0
	THEN
	(SELECT description FROM ad_client where ad_client_id = $P{AD_Client_ID})
	ELSE
	(SELECT o.description FROM ad_client c JOIN ad_org o on c.ad_client_id = o.ad_client_id WHERE c.ad_client_id = $P{AD_Client_ID} and o.ad_org_id = $P{AD_Org_ID})
END) AS cliente,
(select name from c_acctschema where c_acctschema_id = $P{C_AcctSchema_ID}) as esquema,
-- detalle del reporte
i.dateinvoiced as fecha,
(case when (select codsunat from c_doctype where c_doctype_id = i.c_doctype_id) = '14' then to_char(i.dateacct,'dd/mm/yy') else '' end) as fechavcto,	--paymenttermduedate(i.c_paymentterm_id, i.dateinvoiced) as fechavencimiento,
-- comprobante de pago
(select codsunat from c_doctype where c_doctype_id = i.c_doctype_id) as tipo,
(select name from c_doctype where c_doctype_id = i.c_doctype_id) as nombretipo,
--i.serial
(case when ((select codsunat from c_doctype where c_doctype_id = i.c_doctype_id) in('50','53')) then lpad(i.serial,3,'0') else lpad(i.serial,4,'0')end ) as serie,
(case when ((select codsunat from c_doctype where c_doctype_id = i.c_doctype_id) in ('50','52'))then extract(year from i.dateinvoiced) else '0' end) as AnioDUA,
i.documentno as numero,
(select printname from c_doctype where c_doctype_id = i.c_doctype_id) as nombre_tipo,

-- informacion del cliente
getreferencevalue('C_BP_DocTypeI',bp.c_bp_doctypei) as tipo_doci,
(select taxid from c_bpartner where c_bpartner_id = i.c_bpartner_id) as numero_doci,
(select name from c_bpartner where c_bpartner_id = i.c_bpartner_id) as nombre_socio,

-- detalle de montos
dt.docbasetype,
(case when (select codsunat from c_doctype where c_doctype_id = i.c_doctype_id) in ('50','53','91')
then CurrencyConvert((
		(Select sum(linetotalamt)
		From c_invoiceline
		Where C_Invoice_ID= i.C_Invoice_ID and C_Tax_ID=1000104)*100.00/18.00), i.C_Currency_ID, 308, i.dateinvoiced, i.c_conversiontype_id, i.AD_Client_ID, i.AD_Org_ID)
else (case when gettaxvalue( i.c_doctype_id, i.c_invoice_id, 'IGV'::text, 'BASE'::text)!=0
      then round(gettaxvalue( i.c_doctype_id, i.c_invoice_id, 'IGV'::text, 'BASE'::text),2)
      else '0.00'
      end)
end) AS base,

(case when gettaxvalue( i.c_doctype_id, i.c_invoice_id, 'IGV'::text, 'IGV'::text)!=0
then round(gettaxvalue( i.c_doctype_id, i.c_invoice_id, 'IGV'::text, 'IGV'::text),2)
else ( case when (select codsunat from c_doctype where c_doctype_id = i.c_doctype_id) in ('50','53')
	then CurrencyConvert((Select sum(linetotalamt) From c_invoiceline Where C_Invoice_ID= i.C_Invoice_ID and C_Tax_ID=1000104),i.C_Currency_ID, 308, i.dateinvoiced, i.c_conversiontype_id, i.AD_Client_ID, i.AD_Org_ID)
	else '0.00'
	end)
end) AS igv,

gettaxvalue( i.c_doctype_id, i.c_invoice_id, 'IG2'::text, 'BASE'::text) AS base_gravnograv,
gettaxvalue( i.c_doctype_id, i.c_invoice_id, 'IG2'::text, 'IGV'::text) AS igv_gravnograv,
gettaxvalue( i.c_doctype_id, i.c_invoice_id, 'IG3'::text, 'BASE'::text) AS base_nograv,
gettaxvalue( i.c_doctype_id, i.c_invoice_id, 'IG3'::text, 'IGV'::text) AS igv_nograv,
gettaxvalue( i.c_doctype_id, i.c_invoice_id, 'NGR'::text, 'BASE'::text) AS no_gravada,
gettaxvalue( i.c_doctype_id, i.c_invoice_id, 'ISC'::text, 'IGV'::text) AS isc,
gettaxvalue( i.c_doctype_id, i.c_invoice_id, 'OTR'::text, 'IGV'::text) AS otr_tributo,

CASE when dt.DocBaseType= 'APC'
THEN CurrencyConvert(i.grandtotal,i.C_Currency_ID, 308,(Select dateinvoiced from c_invoice where c_invoice_id = i.ref_invoice_id),i.c_conversiontype_id,i.AD_Client_ID, i.AD_Org_ID) * (-1)::numeric
ELSE (case when (select codsunat from c_doctype  where c_doctype_id = i.c_doctype_id) = '50'
	then CurrencyConvert(((Select sum(linetotalamt) From c_invoiceline Where C_Invoice_ID= i.C_Invoice_ID and C_Tax_ID=1000104)*100.00/18.00 + i.grandtotal), i.C_Currency_ID, 308, i.dateinvoiced, i.c_conversiontype_id,i.AD_Client_ID, i.AD_Org_ID)
else CurrencyConvert(i.grandtotal,i.C_Currency_ID, 308, i.dateinvoiced, i.c_conversiontype_id,i.AD_Client_ID, i.AD_Org_ID)
end)END AS total,
currencyrate(i.c_currency_id, 308, i.dateinvoiced, i.c_conversiontype_id, i.ad_client_id, 0) as tipo_cambio ,
--detraccion
''::text AS numero_detra,
null::timestamp AS fecha_detra,
--(SELECT py.checkno FROM c_payment py
--JOIN c_bankaccount bka ON bka.c_bankaccount_id = py.c_bankaccount_id
--WHERE bka.bankaccounttype = 'D' limit 1) as numero_detra,
--(SELECT py.datetrx FROM c_payment py
--JOIN c_bankaccount bka ON bka.c_bankaccount_id = py.c_bankaccount_id
--WHERE bka.bankaccounttype = 'D' limit 1) as fecha_detra,
--referencia

(SELECT dateinvoiced from c_invoice where c_invoice_id = i.ref_invoice_id) as fecharef,
(SELECT codsunat from c_doctype where c_doctype_id in (SELECT c_doctype_id from c_invoice where c_invoice_id = i.ref_invoice_id)) as tiporef,
(SELECT serial from c_invoice where c_invoice_id = i.ref_invoice_id) as serieref,
(SELECT documentno from c_invoice where c_invoice_id = i.ref_invoice_id) as documentref,
u.name as usuario

from c_invoice i
join c_bpartner bp on i.c_bpartner_id = bp.c_bpartner_id
join c_doctype dt on i.c_doctype_id = dt.c_doctype_id
join gl_category gl ON gl.gl_category_id = dt.gl_category_id
join ad_user u on i.salesrep_id = u.ad_user_id

WHERE i.issotrx = 'N'
AND gl.codsunat = '08'
AND i.docstatus in ('CO','CL')
AND coalesce(dt.codsunat,'') !=''
AND dt.codsunat != '02'
AND date(i.dateacct)>=date($P{DateFrom1})
AND date(i.dateacct)<=date($P{DateFrom2})
AND ( CASE WHEN COALESCE($P{AD_Org_ID}, 0) = 0
	THEN true ELSE i.AD_Org_ID = $P{AD_Org_ID} END)
AND (CASE WHEN COALESCE($P{AD_User_ID}, 0)  = 0
            THEN true
            ELSE i.salesrep_id=$P{AD_User_ID}
        END)

ORDER  BY dt.codsunat, i.fact_id]]></queryString>
