	<queryString>
		<![CDATA[SELECT
	i.fact_id as correlativo,
	(select name from ad_client where ad_client_id = i.ad_client_id) AS name,
	(select name from c_period where c_period_id = $P{C_Period_ID}) as periodo,
	(SELECT CASE WHEN $P{AD_Org_ID}=0 THEN
		(SELECT taxid FROM ad_clientinfo where ad_client_id = $P{AD_Client_ID})
	ELSE
		(SELECT taxid FROM ad_orginfo WHERE ad_client_id = $P{AD_Client_ID} and ad_org_id = $P{AD_Org_ID})
	END) AS ruc,
	(SELECT CASE WHEN $P{AD_Org_ID}=0 THEN
		(SELECT description FROM ad_client where ad_client_id = $P{AD_Client_ID})
	ELSE
		(SELECT o.description FROM ad_client c JOIN ad_org o on c.ad_client_id = o.ad_client_id
			WHERE c.ad_client_id = $P{AD_Client_ID} and o.ad_org_id = $P{AD_Org_ID})
	END) AS cliente,
	(select name from c_acctschema where c_acctschema_id =$P{C_AcctSchema_ID}) as esquema,
	-- detalle del reporte
	i.dateinvoiced as fecha,

(case when dtx.taxid = '14' then to_char(i.dateacct,'dd/mm/yy') else '' end) as fechavcto,

	-- comprobante de pago
	dtx.taxid as tipo,
	--i.serial
	(case when dtx.taxid in('50','53','54') then lpad(i.serial,3,'0') else lpad(i.serial,4,'0')end ) as serie,
	(case when dtx.taxid in ('50','52','54') then extract(year from i.dateinvoiced) else '0' end) as AnioDUA,
	i.documentno as numero,
	dt.name as nombre_tipo,
	-- informacion del cliente
	bpt.taxid as tipo_doci,
	bp.taxid as numero_doci,
	bp.name as nombre_socio,
	-- detalle de montos
	dt.docbasetype,

(case 	when gettaxvalue(i.c_doctype_id, i.c_invoice_id, 'NGR'::text, 'BASE'::text) < 0
	then
	(case 	when gettaxvalue( i.c_doctype_id, i.c_invoice_id, 'IGV'::text, 'BASE'::text)!=0
		then
		round(gettaxvalue( i.c_doctype_id, i.c_invoice_id, 'IGV'::text, 'BASE'::text)+ gettaxvalue( i.c_doctype_id, i.c_invoice_id, 'NGR'::text, 'BASE'::text),2)
		else '0.00'
	end)
	else
	(case when dtx.taxid in ('50','52','53','91')
		then
		CurrencyConvert((
			(Select sum(linetotalamt)
			From c_invoiceline
			Where C_Invoice_ID= i.C_Invoice_ID and (C_Tax_ID= 1000103 or C_Tax_ID=1000096))*100.00/18.00),
			i.C_Currency_ID, 308, i.dateinvoiced, i.c_conversiontype_id,i.AD_Client_ID, i.AD_Org_ID)
		else
		(case	when gettaxvalue( i.c_doctype_id, i.c_invoice_id, 'IGV'::text, 'BASE'::text)!=0
			then
			round(gettaxvalue( i.c_doctype_id, i.c_invoice_id, 'IGV'::text, 'BASE'::text),2)
			else '0.00'
		end)
	end)
end)

AS base,
(case 	when gettaxvalue( i.c_doctype_id, i.c_invoice_id, 'IGV'::text, 'IGV'::text)!=0
	then
	round(gettaxvalue( i.c_doctype_id, i.c_invoice_id, 'IGV'::text, 'IGV'::text),2)
		else ( case when dtx.taxid in ('00','50','53','52')
			then
			CurrencyConvert((Select sum(linetotalamt) From c_invoiceline Where C_Invoice_ID= i.C_Invoice_ID and (C_Tax_ID= 1000103 or C_Tax_ID=1000096))
					,i.C_Currency_ID, 308, i.dateinvoiced, i.c_conversiontype_id,i.AD_Client_ID, i.AD_Org_ID)
		else '0.00'
		end
	)
end) AS igv,

	gettaxvalue( i.c_doctype_id, i.c_invoice_id, 'IG2'::text, 'BASE'::text) AS base_gravnograv,
	gettaxvalue( i.c_doctype_id, i.c_invoice_id, 'IG2'::text, 'IGV'::text) AS igv_gravnograv,
	gettaxvalue( i.c_doctype_id, i.c_invoice_id, 'IG3'::text, 'BASE'::text) AS base_nograv,
	gettaxvalue( i.c_doctype_id, i.c_invoice_id, 'IG3'::text, 'IGV'::text) AS igv_nograv,
	gettaxvalue( i.c_doctype_id, i.c_invoice_id, 'NGR'::text, 'BASE'::text) AS no_gravada,
	gettaxvalue( i.c_doctype_id, i.c_invoice_id, 'ISC'::text, 'IGV'::text) AS isc,
	gettaxvalue( i.c_doctype_id, i.c_invoice_id, 'OTR'::text, 'IGV'::text) AS otr_tributo,

CASE when dtx.taxid in('07','97')
	THEN
	CurrencyConvert(i.grandtotal,i.C_Currency_ID, 308,
	(Select dateinvoiced from c_invoice where c_invoice_id = i.ref_invoice_id),
	i.c_conversiontype_id,i.AD_Client_ID, i.AD_Org_ID) * (-1)::numeric
when dtx.taxid ='08'
	THEN
	CurrencyConvert(i.grandtotal,i.C_Currency_ID, 308,
	(Select dateinvoiced from c_invoice where c_invoice_id = i.ref_invoice_id),
	i.c_conversiontype_id,i.AD_Client_ID, i.AD_Org_ID)::numeric


ELSE (case when dtx.taxid in ('50','53','52')
	then CurrencyConvert((
(Select sum(linetotalamt) From c_invoiceline Where C_Invoice_ID= i.C_Invoice_ID and (C_Tax_ID= 1000103 or C_Tax_ID=1000096))*100.00/18.00 + (Select sum(linetotalamt) From c_invoiceline Where C_Invoice_ID= i.C_Invoice_ID and (C_Tax_ID= 1000103 or C_Tax_ID=1000096))),
i.C_Currency_ID, 308, i.dateinvoiced, i.c_conversiontype_id,i.AD_Client_ID, i.AD_Org_ID)
else CurrencyConvert(i.grandtotal,i.C_Currency_ID, 308, i.dateinvoiced, i.c_conversiontype_id,i.AD_Client_ID, i.AD_Org_ID)
	end)END AS total,

(CASE when dtx.taxid in ('07','08') and i.c_currency_id = 100
	THEN round(currencyrate(i.c_currency_id, 308, (Select dateinvoiced from c_invoice ir where ir.c_invoice_id = i.ref_invoice_id),i.c_conversiontype_id, i.ad_client_id, i.ad_org_id),3) else currencyrate(i.c_currency_id, 308, i.dateinvoiced, i.c_conversiontype_id, i.ad_client_id, i.ad_org_id) end) as tipo_cambio ,
        --detraccion
(i.datedetraction) AS fecha_detra,
(i.docdetraction)AS numero_detra,
	(SELECT dateinvoiced from c_invoice where c_invoice_id = i.ref_invoice_id) as fecharef,
	(select taxid from c_documenttax dtx left join c_doctype dt on dtx.c_documenttax_id = dt.c_documenttax_id where dt.c_doctype_id in (SELECT c_doctype_id from c_invoice where c_invoice_id = i.ref_invoice_id)) as tiporef,
	(SELECT serial from c_invoice where c_invoice_id = i.ref_invoice_id) as serieref,
	(SELECT documentno from c_invoice where c_invoice_id = i.ref_invoice_id) as documentref
 from
	c_invoice i
	left join c_bpartner bp on i.c_bpartner_id = bp.c_bpartner_id
	left join c_bp_doctype bpt on bp.c_bp_doctype_id = bpt.c_bp_doctype_id
	left join c_doctype dt on i.c_doctype_id = dt.c_doctype_id
	left join c_documenttax dtx on dt.c_documenttax_id = dtx.c_documenttax_id
	left join gl_bookline bl on i.c_doctype_id = bl.c_doctype_id
	left join gl_book b on bl.gl_book_id = b.gl_book_id
	join gl_category gl ON gl.gl_category_id = dt.gl_category_id
	WHERE i.issotrx = 'N'
	AND b.value = '08'
	AND i.docstatus in ('CO','CL')
	AND coalesce(dt.c_documenttax_id,0) !=0
	AND dtx.taxid != '02'
	AND dtx.taxid != '40'
 	AND
	(CASE WHEN COALESCE($P{C_Period_ID}) = 0 THEN
	true
	ELSE
	EXISTS (SELECT 1 FROM FACT_ACCT f WHERE f.ad_table_id = 318 AND
		f.c_acctschema_id = $P{C_AcctSchema_ID} AND f.c_period_id = $P{C_Period_ID}
		AND f.ad_client_id = $P{AD_Client_ID} AND f.record_id = i.c_invoice_ID) OR
        ( i.dateinvoiced between (select startdate from c_period where c_period_id = $P{C_Period_ID})
         and (select enddate from c_period where c_period_id = $P{C_Period_ID})
	)
	END)
	ORDER  BY i.fact_id, fecha, dtx.taxid]]>
	</queryString>
