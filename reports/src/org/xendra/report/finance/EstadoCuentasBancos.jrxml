	<queryString>
		<![CDATA[select
'X'::CHAR as p_status,
'1990-01-01'::TIMESTAMP WITHOUT TIME ZONE as fecTrans,
''::TEXT as pagdocno,
'' as libro ,
''::VARCHAR as voucherno,
''::VARCHAR(40) as codCuenta,
''::VARCHAR(60)  as tipoDoc,
'' as docMov,
'' as description,
'' as moneda,
0 as egreso,
0 as ingreso,
xx.cbancaria,
(CASE WHEN xx.ingresoDolar-xx.egresoDolar > 0 then xx.ingresoDolar-xx.egresoDolar else null
end) as ingresoDolar,
(CASE WHEN xx.ingresoDolar-xx.egresoDolar < 0 then xx.egresoDolar-xx.ingresoDolar else null end) as egresoDolar,
(CASE WHEN xx.ingresoEuro-xx.egresoEuro > 0 then xx.ingresoEuro-xx.egresoEuro else 0 end) as ingresoEuro,
(CASE WHEN xx.ingresoEuro-xx.egresoEuro < 0 then xx.egresoEuro-xx.ingresoEuro else 0 end) as egresoEuro,
(CASE WHEN xx.ingresoSoles-xx.egresoSoles > 0 then xx.ingresoSoles-xx.egresoSoles 	else 0 	end) as ingresoSoles,
(CASE WHEN xx.ingresoSoles-xx.egresoSoles < 0 then xx.egresoSoles-xx.ingresoSoles 	else 0
end) as egresoSoles,
(CASE WHEN xx.debito-xx.credito > 0 then xx.debito-xx.credito else 0 end) as debito,
(CASE WHEN xx.debito-xx.credito < 0 then xx.credito-xx.debito else 0 end) as credito,
--xx.debito,
--xx.credito,
xx.payment_id,
xx.noContab,
pe.startdate,
pe.enddate

from (
select
bn.name || ' - ' || ba.accountno as cbancaria,

sum(case when cur.cursymbol = '$' then case when p.isreceipt = 'Y' or (coalesce(p.isreceipt,'N') = 'N' and dt.docbasetype = 'BAT' and coalesce(p.dest_payment_id,0) = 0) then p.payamt else 0 end else 0 end ) as ingresoDolar,

sum(case when cur.cursymbol = '$' then case when (coalesce(p.isreceipt,'N') = 'N' and dt.docbasetype != 'BAT') or ( coalesce(p.isreceipt,'N') = 'N' and (dt.docbasetype = 'BAT' and coalesce(p.dest_payment_id,0) != 0)) then p.payamt 	else 0 end else 0 end ) as egresoDolar,

sum(case when cur.cursymbol = 'E' then case when p.isreceipt = 'Y' or (coalesce(p.isreceipt,'N') = 'N' and dt.docbasetype = 'BAT' and coalesce(p.dest_payment_id,0) = 0) then p.payamt else 0 end else 0 end ) as ingresoEuro,

sum(case when cur.cursymbol = 'E' then case when (coalesce(p.isreceipt,'N') = 'N' and dt.docbasetype != 'BAT') or ( coalesce(p.isreceipt,'N') = 'N' and (dt.docbasetype = 'BAT' and coalesce(p.dest_payment_id,0) != 0)) then p.payamt 	else 0 end else 0 end ) as egresoEuro,

sum(case when cur.cursymbol = 'S/.' then 	case when p.isreceipt = 'Y' or (coalesce(p.isreceipt,'N') = 'N' and dt.docbasetype = 'BAT' and coalesce(p.dest_payment_id,0) = 0) then p.payamt else 0 end else 0 end ) as ingresoSoles,

sum(case when cur.cursymbol = 'S/.' then 	case when (coalesce(p.isreceipt,'N') = 'N' and dt.docbasetype != 'BAT') or ( coalesce(p.isreceipt,'N') = 'N' and (dt.docbasetype = 'BAT' and coalesce(p.dest_payment_id,0) != 0)) then p.payamt 	else 0 end else 0 end ) as egresoSoles,

sum (case when coalesce(fa.fact_id,'') != '' then fa.amtacctdr when coalesce(fa2.fact_id,'')!=''
then fa2.amtacctdr else null end ) as debito,

sum (case when coalesce(fa.fact_id,'') != '' then fa.amtacctcr when coalesce(fa2.fact_id,'')!=''
then fa2.amtacctcr else null end) as credito,

0 as payment_id,
-- Cuenta los no contabilizados

sum((case when coalesce(fa.fact_acct_id,0)=0 and coalesce(fa2.fact_acct_id,0)=0 then 1 else 0
end) ) as noContab

from c_payment p
left join c_doctype dt on p.c_doctype_id = dt.c_doctype_id

left join fact_acct fa on fa.fact_acct_id = (select min(ff.fact_acct_id) from fact_acct ff where ff.ad_table_id = 735
and record_id = (select c_allocationhdr_id from c_allocationline where c_payment_id=p.c_payment_id limit 1)group by ff.record_id)

left join fact_acct fa2 on fa2.fact_acct_id = (select min(ff.fact_acct_id) from fact_acct ff where ff.ad_table_id = 335
and (ff.record_id = p.c_payment_id
or ff.record_id = p.dest_payment_id) group by ff.record_id)

left join c_bankaccount ba on ba.c_bankaccount_id = p.c_bankaccount_id
left join c_bank bn on bn.c_bank_id = ba.c_bank_id
left join c_currency cur on cur.c_currency_id = p.c_currency_id
left join c_period pe on pe.c_period_id =  $P{C_Period_ID}

where p.isactive='Y'
and p.docstatus != 'DR'
and p.ad_client_id = $P{AD_Client_ID}
and p.ad_org_id= $P{AD_Org_ID}
and p.datetrx < pe.startdate
and (case when coalesce($P{C_BankAccount_ID},0) = 0 then true else p.c_bankaccount_id = $P{C_BankAccount_ID} end)
group by cbancaria,payment_id
) xx

left join c_period pe on pe.c_period_id = $P{C_Period_ID}

union
(
select
(
case when p.docstatus != 'CO' and p.docstatus != 'CL' then 'B' else null end ) as p_status,
p.datetrx as fecTrans,
substring(dt.name,1,3) || '-' || p.documentno as pagdocno,
gb.value as libro ,

(case when coalesce(fa.fact_id,'') != '' then fa.fact_id when coalesce(fa2.fact_id,'')!=''
then fa2.fact_id else null end) as voucherno,
ev.value as codCuenta,
dt.name as tipoDoc,

(case when coalesce(p.checkno,'') != '' and p.checkno != '0' then 'CH-' || p.checkno when coalesce(boe.documentno,'') != '' then 'LT-' || boe.documentno else '' end ) as docMov,
p.description,
cur.cursymbol as moneda,

(case when (coalesce(p.isreceipt,'N') = 'N' and dt.docbasetype != 'BAT') or ( coalesce(p.isreceipt,'N') = 'N' and (dt.docbasetype = 'BAT' and coalesce(p.dest_payment_id,0) != 0)) then p.payamt else 0 end) as egreso,

(case when p.isreceipt = 'Y' or (coalesce(p.isreceipt,'N') = 'N' and dt.docbasetype = 'BAT' and coalesce(p.dest_payment_id,0) = 0) then p.payamt else 0 end) as ingreso,
bn.name || ' - ' || ba.accountno as cbancaria,

(case when cur.cursymbol = '$' then case when p.isreceipt = 'Y' or (coalesce(p.isreceipt,'N') = 'N' and dt.docbasetype = 'BAT' and coalesce(p.dest_payment_id,0) = 0) then p.payamt else 0 end
else 0 end) as ingresoDolar,

(case when cur.cursymbol = '$' then case when (coalesce(p.isreceipt,'N') = 'N' and dt.docbasetype != 'BAT') or ( coalesce(p.isreceipt,'N') = 'N' and (dt.docbasetype = 'BAT' and coalesce(p.dest_payment_id,0) != 0)) then p.payamt 	else 0 end else 0 end) as egresoDolar,

(case when cur.cursymbol = 'E' then case when p.isreceipt = 'Y' or (coalesce(p.isreceipt,'N') = 'N' and dt.docbasetype = 'BAT' and coalesce(p.dest_payment_id,0) = 0) then p.payamt else 0 end
else 0 end ) as ingresoEuro,

(case when cur.cursymbol = 'E' then case when (coalesce(p.isreceipt,'N') = 'N' and dt.docbasetype != 'BAT') or ( coalesce(p.isreceipt,'N') = 'N' and (dt.docbasetype ='BAT' and coalesce(p.dest_payment_id,0) != 0)) then p.payamt 	else 0 end else 0 end) as egresoEuro,

(case when cur.cursymbol = 'S/.' then case when p.isreceipt = 'Y' or (coalesce(p.isreceipt,'N') = 'N' and dt.docbasetype = 'BAT' and coalesce(p.dest_payment_id,0) = 0) then p.payamt else 0 end
else 0 end ) as ingresoSoles,

(case when cur.cursymbol = 'S/.' then case when (coalesce(p.isreceipt,'N') = 'N' and dt.docbasetype!='BAT') or ( coalesce(p.isreceipt,'N') = 'N' and (dt.docbasetype = 'BAT' and coalesce(p.dest_payment_id,0) != 0)) then p.payamt 	else 0 end else 0 end) as egresoSoles,

(case when coalesce(fa.fact_id,'') != '' then fa.amtacctdr when coalesce(fa2.fact_id,'')!='' then fa2.amtacctdr else null end) as debito,

(case when coalesce(fa.fact_id,'') != '' 	then fa.amtacctcr when coalesce(fa2.fact_id,'')!=''
then fa2.amtacctcr else null end) as credito,
p.c_payment_id as payment_id,
0 as noContab,
pe.startdate,
pe.enddate

from c_payment p
left join fact_acct fa on fa.fact_acct_id = (select min(ff.fact_acct_id) from fact_acct ff where ff.ad_table_id = 735 and record_id = (select c_allocationhdr_id from c_allocationline where c_payment_id=p.c_payment_id limit 1) group by ff.record_id)

left join fact_acct fa2 on fa2.fact_acct_id = (select min(ff.fact_acct_id) from fact_acct ff where ff.ad_table_id = 335 and (ff.record_id = p.c_payment_id or ff.record_id = p.dest_payment_id)

group by ff.record_id)

-- Pendejada para detectar el voucher en transferencias fa es origen y fa2 es destino nunca ambos tendran valor.

left join c_elementvalue ev on ev.c_elementvalue_id = coalesce(fa.account_id,0)+coalesce(fa2.account_id,0)
left join gl_bookline bl on bl.c_doctype_id = p.c_doctype_id and bl.ad_table_id = 335
left join gl_book gb on gb.gl_book_id = bl.gl_book_id
left join c_doctype dt on dt.c_doctype_id = p.c_doctype_id
left join c_bankaccount ba on ba.c_bankaccount_id = p.c_bankaccount_id
left join c_bank bn on bn.c_bank_id = ba.c_bank_id
left join c_boe boe on boe.c_boe_id = p.c_boe_id
left join c_currency cur on cur.c_currency_id = p.c_currency_id
left join c_period pe on pe.c_period_id = $P{C_Period_ID}

where p.isactive='Y'
and p.docstatus != 'DR'
and p.ad_client_id = $P{AD_Client_ID}
and p.ad_org_id=$P{AD_Org_ID}
and p.datetrx between pe.startdate and pe.enddate
and (case when coalesce($P{C_BankAccount_ID},0) = 0 then true else p.c_bankaccount_id = $P{C_BankAccount_ID}	end)
)
order by cbancaria,fectrans,payment_id]]>
