	<queryString>
		<![CDATA[select
p.value,
p.name,
i.serial,
i.documentno,
i.dateinvoiced,
il.m_product_id,
il.qtyentered,
il.priceentered

from c_invoice i
left join c_invoiceline il on i.c_invoice_id = il.c_invoice_id
left join m_product p on il.m_product_id = p.m_product_id

where
date (dateinvoiced) >= date($P{DateInvoiced1})
and date (dateinvoiced) <= date($P{DateInvoiced2})
and il.m_product_id = $P{M_Product_ID}
and issotrx = 'Y'
and i.ad_client_id = $P{AD_Client_ID}

order by priceentered, dateinvoiced, serial, documentno]]>
	</queryString>
