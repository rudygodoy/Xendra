sum(ohmlux_mov)+sum(dalsac_mov)+sum(warrant_mov) total_stock,


sum(porrecibir) as porrecibir,
sum(porentregar) as porentregar,
sum(porrecibir)-sum(porentregar) as deficit,

sum(porrecibir_0015) as porrecibir_0015,
sum(porrecibir_1530) as porrecibir_1530,
sum(porrecibir_3060) as porrecibir_3060,
sum(porrecibir_60) as porrecibir_60,

