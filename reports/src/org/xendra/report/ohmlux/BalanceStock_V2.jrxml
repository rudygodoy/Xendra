	and date(datepromised)-date(now()) > 30 and date(datepromised)-date(now()) <= 60
	then max(oqtyenterd)-sum(qtyentered)
	else 0
end
) as porrecibir_3060,
(
case when min(issotrx) = 'Y' and max(oqtyenterd)-sum(qtyentered) >= 0 and min(docstatus) != 'CL'
	and date(datepromised)-date(now()) > 60
	then max(oqtyenterd)-sum(qtyentered)
	else 0
end
) as porentregar_60,
(
case when min(issotrx) = 'N' and max(oqtyenterd)-sum(qtyentered) >= 0 and min(docstatus) != 'CL'
	and date(datepromised)-date(now()) > 60
	then max(oqtyenterd)-sum(qtyentered)
	else 0
end
) as porrecibir_60,
sum(ohmlux_mov)  as ohmlux_mov,
sum(dalsac_mov) as dalsac_mov,
sum(warrant_mov) as warrant_mov,
sum(otros_mov) as otros_mov,
min(issotrx)
from(

select
ol.datepromised,
il.m_product_id as product_id,
il.c_orderline_id,
sum(il.qtyentered) as qtyentered,
max(ol.qtyentered) as oqtyenterd,
(
case when max(ol.qtyentered)-sum(il.qtyentered) >= 0 and min(ord.docstatus) != 'CL'
	then max(ol.qtyentered)-sum(il.qtyentered)
	else 0
end
) as debeser,
(
	case when io.m_warehouse_id = 1000095
		then (
			case when substring(io.movementtype,2) = '-'
				then -1*sum(il.qtyentered)
			else
				sum(il.qtyentered)
			end

		)
	else
		0
	end
) as OHMLUX_MOV,
(
	case when io.m_warehouse_id = 1000093
		then (
			case when substring(io.movementtype,2) = '-'
				then -1*sum(il.qtyentered)
			else
				sum(il.qtyentered)
			end

		)
	else
		0
	end
) as DALSAC_MOV,
(
	case when io.m_warehouse_id = 1000094
		then (
			case when substring(io.movementtype,2) = '-'
				then -1*sum(il.qtyentered)
			else
				sum(il.qtyentered)
			end

