col.datepromised,
(
case when issotrx = 'N'
	then qtyordered
	else 0
end
) as porrecibir,
(
case when issotrx = 'Y'
	then qtyordered
	else 0
end
) as porentregar,
(
case when issotrx = 'Y'
	and (date(col.datepromised)-date(now())) <= 15
	then qtyordered
	else 0
end
) as porentregar_0015,
(
case when issotrx = 'Y'
	and (date(col.datepromised)-date(now())) > 15 and (date(col.datepromised)-date(now())) <= 30
	then qtyordered
	else 0
end
) as porentregar_1530,
(
case when issotrx = 'Y'
	and (date(col.datepromised)-date(now())) > 30 and (date(col.datepromised)-date(now())) <= 60
	then qtyordered
	else 0
end
) as porentregar_3060,
(
case when issotrx = 'Y'
	and (date(col.datepromised)-date(now())) > 60
	then qtyordered
	else 0
end
) as porentregar_60,
(
case when issotrx = 'N'
	and (date(col.datepromised)-date(now())) <= 15
	then qtyordered
	else 0
end
) as porrecibir_0015,
(
case when issotrx = 'N'
	and (date(col.datepromised)-date(now())) > 15 and (date(col.datepromised)-date(now())) <= 30
	then qtyordered
	else 0
end
) as porrecibir_1530,
(
case when issotrx = 'N'
	and (date(col.datepromised)-date(now())) > 30 and (date(col.datepromised)-date(now())) <= 60
	then qtyordered
	else 0
end
) as porrecibir_3060,
(
case when issotrx = 'N'
	and (date(col.datepromised)-date(now())) > 60
	then qtyordered
	else 0
end
) as porrecibir_60


from c_orderline col
left join c_order co on co.c_order_id = col.c_order_id
left join m_inoutline iol on iol.c_orderline_id = col.c_orderline_id
where iol.c_orderline_id IS NULL
and co.docstatus != 'CL' and co.docstatus != 'DR' and co.docstatus != 'IN'  and co.docstatus != 'VO'
and co.c_bpartner_id != 1012821 and col.m_product_id is not null

) xx
group by product_id

--
-- Ahora las transferencias
----
union


select
product_id,
sum(ohmlux)  as ohmlux,
sum(dalsac) as dalsac,
sum(warrant) as warrant,
sum(otros) as otros,
sum(ohmlux)+sum(dalsac)+sum(warrant) as total_stock,

0 as porrecibir,
0 as porentregar,
0 as deficit,

0 as porrecibir_0015,
0 as porrecibir_1530,
0 as porrecibir_3060,
0 as porrecibir_60,

0 as porentregar_0015,
0 as porentregar_1530,
0 as porentregar_3060,
0 as porentregar_60,

0+(sum(ohmlux)+sum(dalsac)+sum(warrant)) as balance

from (
select
m_product_id as product_id,
(
	case when wo.m_warehouse_id = 1000095
		then -1*ml.movementqty
		else
		(
			case when wd.m_warehouse_id = 1000095
				then ml.movementqty
				else 0
			end

		)

	end

) as OHMLUX,
(
	case when wo.m_warehouse_id = 1000093
		then -1*ml.movementqty
		else
		(
			case when wd.m_warehouse_id = 1000093
				then ml.movementqty
				else 0
			end

		)

	end

) as DALSAC,
(
	case when wo.m_warehouse_id = 1000094
		then -1*ml.movementqty
		else
		(
			case when wd.m_warehouse_id = 1000094
				then ml.movementqty
				else 0
			end

		)

	end

) as WARRANT,
(
	case when wo.m_warehouse_id = 1000096 or wo.m_warehouse_id = 1000097 or wo.m_warehouse_id = 1000098
		then -1*ml.movementqty
		else
		(
			case when wd.m_warehouse_id = 1000098 or wd.m_warehouse_id = 1000096 or wd.m_warehouse_id = 1000097
				then ml.movementqty
				else 0
			end
		)

	end

) as OTROS

from m_movement mio
join m_movementline ml on ml.m_movement_id=mio.m_movement_id and ml.isactive  = 'Y' --and ml.m_product_id = 1017928
join m_warehouse wo on wo.m_warehouse_id = (select m_warehouse_id from m_locator loc where loc.m_locator_id = ml.m_locator_id)
join m_warehouse wd on wd.m_warehouse_id = (select m_warehouse_id from m_locator loc where loc.m_locator_id = ml.m_locatorto_id)
where mio.isactive  = 'Y' and mio.docstatus != 'DR' and mio.docstatus != 'IN' and mio.docstatus != 'VO'
and mio.ad_client_id = 1000026 and mio.ad_org_id=1000069
) xx
group by product_id

--
-- Ahora inventarios
union

select
product_id,
sum(ohmlux)  as ohmlux,
sum(dalsac) as dalsac,
sum(warrant) as warrant,
sum(otros) as otros,
sum(ohmlux)+sum(dalsac)+sum(warrant) as total_stock,

0 as porrecibir,
0 as porentregar,
0 as deficit,

0 as porrecibir_0015,
0 as porrecibir_1530,
0 as porrecibir_3060,
0 as porrecibir_60,

0 as porentregar_0015,
0 as porentregar_1530,
0 as porentregar_3060,
0 as porentregar_60,

0+(sum(ohmlux)+sum(dalsac)+sum(warrant)) as balance

from (

select
m_product_id as product_id,
qtycount-qtybook as qtyentered,
(
	case when i.m_warehouse_id = 1000095
		then qtycount-qtybook
	else
		0
	end
) as OHMLUX,
(
	case when i.m_warehouse_id = 1000093
		then qtycount-qtybook
	else
		0
	end
) as DALSAC,
(
	case when i.m_warehouse_id = 1000094
		then qtycount-qtybook
	else
		0
	end
) as WARRANT,
(
	case when i.m_warehouse_id = 1000096 or i.m_warehouse_id = 1000097 or i.m_warehouse_id = 1000098
		then qtycount-qtybook
	else
		0
	end
) as OTROS

from m_inventoryline il
join m_inventory i on il.m_inventory_id = i.m_inventory_id and i.isactive  = 'Y' and i.docstatus != 'DR' and i.docstatus != 'IN' and i.docstatus != 'VO'
where il.isactive = 'Y'

) xx
group by product_id

--------------------
-- Produccion
--------------------
union
select
product_id,
sum(ohmlux)  as ohmlux,
sum(dalsac) as dalsac,
sum(warrant) as warrant,
sum(otros) as otros,
sum(ohmlux)+sum(dalsac)+sum(warrant) as total_stock,

0 as porrecibir,
0 as porentregar,
0 as deficit,

0 as porrecibir_0015,
0 as porrecibir_1530,
0 as porrecibir_3060,
0 as porrecibir_60,

0 as porentregar_0015,
0 as porentregar_1530,
0 as porentregar_3060,
0 as porentregar_60,

0+(sum(ohmlux)+sum(dalsac)+sum(warrant)) as balance

from (

select
il.m_product_id as product_id,
movementqty as qtyentered,
(
	case when w.m_warehouse_id = 1000095
		then movementqty
	else
		0
	end
) as OHMLUX,
(
	case when w.m_warehouse_id = 1000093
		then movementqty
	else
		0
	end
) as DALSAC,
(
	case when w.m_warehouse_id = 1000094
		then movementqty
	else
		0
	end
) as WARRANT,
(
	case when w.m_warehouse_id = 1000096 or w.m_warehouse_id = 1000097 or w.m_warehouse_id = 1000098
		then movementqty
	else
		0
	end
) as OTROS,
*
from m_productionline  il
join m_productionplan i on il.m_productionplan_id = i.m_productionplan_id and i.isactive  = 'Y'
join m_production pp on pp.m_production_id = i.m_production_id
JOIN m_warehouse w ON w.m_warehouse_id = (( SELECT loc.m_warehouse_id FROM m_locator loc
						WHERE loc.m_locator_id = il.m_locator_id))
where il.isactive='Y' and (pp.docstatus = ANY (ARRAY['CO'::bpchar, 'CL'::bpchar])) and pp.isactive='Y'

) xx
group by product_id
) total_agrupador
group by product_id

) datos_producto
     left join m_product prod on product_id = prod.m_product_id
     left join "m_product_group" m_product_group ON prod."m_product_group_id" = m_product_group."m_product_group_id"
     left join "m_product_brand" m_product_brand ON prod."m_product_brand_id" = m_product_brand."m_product_brand_id"
     left join "m_product_type" m_product_type ON prod."m_product_type_id" = m_product_type."m_product_type_id"
     left join "m_product_generic" m_product_generic ON prod."m_product_generic_id" = m_product_generic."m_product_generic_id"
	where prod.ad_client_id = 1000026 and prod.isactive = 'Y' and prod.issummary ='N'
    AND
     (CASE WHEN COALESCE($P{M_Product_Group_ID}, 0)  = 0
        THEN true
        ELSE prod.m_product_group_id = $P{M_Product_Group_ID}
    END)
    AND
     (CASE WHEN COALESCE($P{M_Product_Type_ID}, 0)  = 0
        THEN true
        ELSE prod.m_product_type_id = $P{M_Product_Type_ID}
    END)
    AND
     (CASE WHEN COALESCE($P{M_Product_Brand_ID}, 0)  = 0
        THEN true
        ELSE prod.m_product_brand_id = $P{M_Product_Brand_ID}
    END)
    AND
     (CASE WHEN COALESCE($P{M_Product_Generic_ID}, 0)  = 0
        THEN true
        ELSE prod.m_product_generic_id = $P{M_Product_Generic_ID}
    END)
order by
m_product_group."name",
m_product_type."name",
m_product_brand."name",
m_product_generic."name",
prod.name]]>
