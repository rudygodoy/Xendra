union

select
m_product_id as product_id,
0 as ohmlux,
0 as dalsac,
0 as warrant,
0 as otros,
0 as total_stock,
sum(porrecibir) as porrecibir,
sum(porentregar) as porentregar,
sum(porentregar)-sum(porrecibir) as deficit,

