sum(porentregar_60) as porentregar_60,

(sum(porrecibir)-sum(porentregar))+(sum(ohmlux_mov)+sum(dalsac_mov)+sum(warrant_mov)) as balance


from (

select
datepromised,
product_id,
c_orderline_id,
sum(qtyentered) as qtyentered,
max(oqtyenterd) as oqtyenterd,
--sum(debeser) as debeser,
